    <!-- Page Content -->
    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">All Athletes in Olympics</h1>
                </div>

                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3>This table shows all the athletes that have participated in Olympics.</h3>
                        </div>
                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                            <tr>
                                <th>Name</th>
                                <th>Sex</th>
                                <th>Country</th>
                            </tr>
                            </thead>
                            <tbody>
                            <% data.forEach(function(name){%>
                            <tr>
                                <td><%= name[0]%></td>
                                <td><%= name[2]%></td>
                                <td><%= name[3]%></td>
                            </tr>
                            <%})%>
                            </tbody>
                        </table>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
